<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Modelity</th>
      <th scope="col">Details</th>
    </tr>
  </thead>
  <tbody>
    <% @psychologists.each_with_index do |psychologist, index| %>
      <tr>
        <td><%= psychologist.first_name %></td>
        <td><%= psychologist.last_name %></td>
        <td><%= psychologist.psychologist_profile&.modelity %></td>
        <td><%= link_to 'More details', psychologist_path(psychologist.id), class: 'btn btn-success'  %></td>
      </tr>
    <% end %>
  </tbody>
</table>
